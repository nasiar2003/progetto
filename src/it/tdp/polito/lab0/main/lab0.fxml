<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane prefHeight="360.0" prefWidth="306.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="it.tdp.polito.lab0.lab0Controller">
   <top>
      <VBox prefHeight="75.0" prefWidth="296.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="39.0" prefWidth="402.0">
               <children>
                  <Label alignment="CENTER" text="Lab 0 - Lista di Parole" />
               </children>
            </HBox>
            <HBox prefHeight="35.0" prefWidth="296.0" spacing="5.0">
               <children>
                  <Label text="Parola" />
                  <TextField fx:id="textField" prefHeight="25.0" prefWidth="193.0" />
