               </children>
            </HBox>
         </children>
      </VBox>
   </top>
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
   <center>
      <VBox prefHeight="232.0" prefWidth="296.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="textArea" editable="false" prefHeight="251.0" prefWidth="296.0" />
         </children>
      </VBox>
   </center>
   <bottom>
      <VBox alignment="TOP_RIGHT" prefHeight="39.0" prefWidth="296.0" BorderPane.alignment="TOP_RIGHT">
         <children>
            <Button fx:id="cancelBtn" mnemonicParsing="false" onAction="#doCancel" text="Cancella" />
